a = int(input())

z = []

while sum(z) < a:
    x = len(input())
    if x > 6:
        print('bom sopro!')
        z.append(1)
    elif x >= 4:
        print('um pouco mais de força no sopro!')
    else:
        print('precisa de muito mais força no sopro!')

print('Parabéns para pelo seu aniversário de',a,'anos!')